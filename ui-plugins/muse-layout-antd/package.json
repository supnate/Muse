{
  "name": "@ebay/muse-layout-antd",
  "version": "1.3.2",
  "repository": "",
  "type": "module",
  "muse": {
    "devConfig": {
      "app": "musemanager",
      "env": "staging"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:dev": "cross-env NODE_ENV=development vite build --mode development",
    "build:test": "cross-env MUSE_TEST_BUILD=true NODE_ENV=production vite build --mode e2e-test",
    "gen-muse-doc": "typedoc",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@ebay/muse-core": "^1.0.45",
    "@ebay/muse-lib-antd": "1.3.2",
    "@ebay/muse-lib-react": "1.3.2",
    "@ebay/muse-typedoc-plugin": "^1.0.6",
    "@ebay/muse-vite-plugin": "^1.0.55",
    "@eslint/js": "^9.5.0",
    "@jridgewell/gen-mapping": "^0.1.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "^1.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "globals": "^15.6.0",
    "jest-canvas-mock": "^2.5.2",
    "js-plugin": "1.1.0",
    "jsdom": "^24.0.0",
    "less": "^4.2.0",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-redux": "8.1.2",
    "semver": "7.3.7",
    "tailwindcss": "^3.4.3",
    "typedoc": "^0.25.13",
    "vite": "^5.4.11",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "autoprefixer": "^10.4.19",
    "react-dom": "18.2.0"
  }
}
